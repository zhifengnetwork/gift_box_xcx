page {
  height: 100%;
  width: 100%;
  overflow-x: hidden;
  background: #f7f7f7;
}

.img_header {
  width: 100%;
  display: flex;
  justify-content: center;
  background: white;
}

.addidas {
  width: 110rpx;
  height: 110rpx;
  margin-top: 200rpx;
}

.text_header {
  background: white;
  font-size: 31rpx;
  padding: 20rpx 40rpx 0 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #333;
}

.centert_pic {
  background: white;
  padding: 40rpx 0rpx;
  display: flex;
  justify-content: center;
}

.cen_pic {
  width: 50rpx;
  height: 35rpx;
}

.text_detail {
  background: white;
  font-size: 31rpx;
  padding: 20rpx 40rpx 0 40rpx;
  color: #333;
}

.paixu_ear {
  text-align: right;
  font-size: 30rpx;
  margin: 30rpx 0;
}

.pai_ear {
  display: inline;
  padding-right: 40rpx;
  box-sizing: border-box;
}

.pai {
  display: inline-block;
}

.san_pic {
  width: 20rpx;
  height: 15rpx;
  margin-left: 20rpx;
}

.shop_ear {
  background: white;
  padding: 40rpx 40rpx;
  box-sizing: border-box;
}

.shoot_pic {
  width: 309rpx;
  height: 309rpx;
}

.didi {
  width: 309rpx;
  height: 510rpx;
  display: inline-block;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  background: #f7f7f7;
}

.didi:nth-child(even) {
  float: right;
}

.desc {
  font-size: 32rpx;
  color: #ccc;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin: 11rpx 0rpx;
  padding: 0 15rpx;
  box-sizing: border-box
}

.price {
  font-size: 32rpx;
  color: #ccc;
  padding: 0 15rpx;
  box-sizing: border-box
}

.xiala_ear {
  width: 100%;
  position: absolute;
  top: 600rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  background: white;
}

.xiala_ear_item {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #f5f5f5;
  box-sizing: border-box;
}

.left_desc {
  display: inline-block;
  vertical-align: center;
  padding-left: 50rpx;
}

.right_desc {
  width: 0rpx;
  height: 40rpx;
  padding-right: 50rpx;
  display: inline-block;
  vertical-align: center;
  float: right;
  margin-top: 33rpx;
  margin-right: 50rpx;
}

.dui_pic {
  width: 100%;
  height: 100%;
}

.checkbox {
  width: 0rpx;
  height: 45rpx;
  padding-right: 50rpx;
  display: inline-block;
  vertical-align: center;
  float: right;
  margin-top: 28rpx;
  margin-right: 50rpx;
  background: url("https://giftbox.zhifengwangluo.com/public/images/check.png");
  background-size: 100% 100%;
}
.name{
  height: 37rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  padding: 0 15rpx;
  box-sizing: border-box
}
