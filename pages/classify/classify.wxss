/* 字母检索样式 */
.searchLetter {
  position: fixed;
  right: 0;
  width: 50rpx;
  text-align: center;
  justify-content: center;
  display: flex;
  flex-direction: column;
  color: #666;
  z-index: 1;
}

.searchLetter view {
  /* margin-top: 20rpx; */
}

.touchClass {
  background-color: #fff;
  position: fixed;
  top: 0;
  bottom: 0;
  margin: auto 0;
}

.showSlectedLetter {
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 50%;
  left: 50%;
  margin: -100rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
  font-size: 52rpx;
  z-index: 1;
}

.selection {
  display: flex;
  width: 100%;
  flex-direction: column;
  margin-top: 10rpx;
}

.item_letter {
  display: flex;
  height: 40rpx;
  padding-left: 34rpx;
  align-items: center;
  font-size: 30rpx;
  color: black;
}

.item_city {
  display: flex;
  background-color: #fff;
  height: 120rpx;
  padding-left: 34rpx;
  align-items: center;
  font-size: 24rpx;
  color: #666;
}

