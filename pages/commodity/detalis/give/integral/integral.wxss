.integral_top {
  height: 116rpx;
}

.integral_t {
  height: 65rpx;
  margin: 88rpx;
}

.integral_t>text {
  width: 150rpx;
  font-size: 26rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  float: left;
  line-height: 65rpx;
  text-align: right;
  margin-right: 20rpx;
}

.integral_input {
  height: 65rpx;
  background: rgba(240, 242, 245, 1);
  border-radius: 8rpx;
  float: left;
  font-size: 26rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 65rpx;
  padding-left: 20rpx;
}

.integral_button {
  position: fixed;
  bottom: 33rpx;
  left: 90rpx;
  border-radius: 50rpx;
  width: 570rpx;
  height: 80rpx;
  background: #000;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 22rpx;
}

/* 弹窗 */

.integral_pop-upbox {
  z-index: 2;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.integral_pop-up {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
}

.pop-up_box {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -70%);
  text-align: center;
  width: 513rpx;
  height: 636rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20px;
}

.pop-up_boximg {
  margin: 127rpx 120rpx 0; 
  width: 273rpx;
  height: 193rpx;
}

.pop-up_view {
  font-size: 35rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 100rpx;
}

.pop-up_text {
  font-size: 18rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.pop-up_dian{
  height: 12rpx;
}

.pop-up_dian>.pop-up_dianl{
  width:12rpx;
height:12rpx;
background: #000;
border-radius:50%;
display: inline-block;
}
.pop-up_dian>text{
  display: inline-block;
  width:12rpx;
height:12rpx;
background:rgba(197,195,195,1);
border-radius:12rpx;
margin-left: 10rpx;
}
.close {
  width: 90rpx;
  height: 90rpx;
  background: #000;
  border-radius: 50%;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
  position: absolute;
  top: 70%;
  left: 50%;
  transform: translate(-50%);
  z-index: 2;
}
