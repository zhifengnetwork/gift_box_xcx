page{
  /* background: #000; */
  position: relative
}
.bg{
  min-height: 100vh;
  background-color: rgba(0,0,0,0.9)
}
.bg_color{
  width: 100%;
  /* min-height: 100vh; */
  /* height: 100%; */
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
  filter: blur(10px);
}
.sousou_ear{
  margin-left: 8%;
  width:671rpx;
  height:60rpx;
  background:rgba(67,67,67,1); 
  border-radius:30rpx;
  position: relative;
  top:160rpx;
}
.sousou{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  margin-left: 40rpx;
  top: 13rpx;
  z-index: 2
}
.find{
  position: absolute;
  top: 0;
  left: 100rpx;
  width: 65%;
  height: 60rpx;
  font-size: 25rpx;
  color: #fff;
}
.close_pic{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 13rpx;
  right: 20rpx;
  z-index: 2
}
.swiper-tab{
  margin-top: 200rpx
}
.about_page{
  margin: 0 10px;
}
.swiper-tab{
    text-align: center;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    font-size: 16px;
    position: sticky;
    top: 0;
}
.swiper_img{
  width: 90rpx;
  height: 90rpx;
  background:linear-gradient(0deg,rgba(156,145,142,1) 0%,rgba(77,77,76,1) 100%);
  border-radius:50%;
  overflow: hidden;
  margin-left: 50%;
  transform: translate(-50%);
  margin-top: 10rpx;
  z-index: 1;
}
.siteimg{
  width: 40rpx;
  height: 40rpx;
  overflow: hidden;
  margin-left: 50%;
  margin-top: 50%;
  transform: translate(-50%,-50%);
}
.siteimg image{
  width: 100%;
  height: 100%;
}
.swiper-tab-item{
    flex: 1;
    font-size:23rpx;
    font-family:SourceHanSansCN-Normal;
    font-weight:400;
    color:rgba(153,153,153,1);
    line-height:70rpx;
    border-top: 1rpx solid rgba(60,55,52,1);
    border-bottom: 1rpx solid rgba(60,55,52,1);
}
.active{
    color: #fff;
    border-bottom: 4rpx solid #Fff;
    font-size: 35;
    font-weight: bold;
}
.juzhong{
  margin: 0 auto;
}
.domain{
  background-color: #fff;
  height: 100%;
  margin:0 10px;
}
.show{
  display: block;
}
.hidden{
  display: none;
}
/* ���ӱ�ǩ */
.tianjia{
  margin: 40rpx;
  height:110rpx;
  background:rgba(243, 238, 238, 0.2);
  border-radius:15px;
  overflow: hidden;
  position: relative;
}
.tianjia_l{
  float: left;
  width:65rpx;
  height:65rpx;
  background:rgba(23,25,23,0.6);
  border-radius:10px;
  margin-top: 22rpx;
  margin-left: 22rpx;
}
.tianjia_limg{
  width: 32rpx;
  height: 32rpx;
  margin-top: 15rpx;
  margin-left: 15rpx;
  overflow: hidden;
}
.tianjia_limg image{
  width: 100%;
  height: 100%;
  display: block;
}
.tianjia_r{
  float: left;
  margin-left: 40rpx;
}
.tianjia_r view{
  font-size:25rpx;
  line-height: 10rpx;
  margin-top: 30rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.tianjia_r text{
  font-size:21rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:50rpx;
}
/* //////////////////////////// */
.tab_t{
  font-size:22rpx;
  margin: 0 40rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(221,221,221,1);
  line-height:80rpx;
}
.tab_c{
  margin: 0 40rpx;
  overflow: hidden;
  background:rgba(67,67,67,0.2);
  border-radius:15rpx;
  margin-bottom: 10rpx;
}
.tab_cl{
  float: left;
  width:60rpx;
  height:60rpx;
  background:rgba(0,0,0,0.6);
  border-radius:10rpx;
  margin: 20rpx;
  overflow: hidden;
}
.tab_cl image{
  width: 100%;
  height: 100%;
  display: block;
}
.tab_climg{
  width:36rpx;
  height:44rpx;
  margin: 8rpx 12rpx;
  overflow: hidden;
}
.tab_climg image{
  width: 100%;
  height: 100%;
}
.tab_cr{
  float: left;
  margin-left: 10rpx;
  width: 80%;
}
.tab_cr view{
  margin-top: 20rpx;
  font-size:25rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  /* line-height: 10rpx; */
  color:rgba(255,255,255,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.tab_cr text{
  font-size:21rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.more{
  /* background: #000; */
  text-align: center;
  position: relative;
  height: 100rpx;
}
.more text{
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  font-size:25rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:100rpx;
}
.xiaimg{
  width: 20rpx;
  height: 10rpx;
  overflow: hidden;
  position: absolute;
  left: 58%;
  top: 50%;
}
.xiaimg image{
  width: 100%;
  height: 100%;
  display: block;
}