page{
  background: #000;
}
.imgbox{
  height: 560rpx;
  margin-top: 200rpx;
  background: #fff;
}
.select{
  height: 250rpx;
}
.select_l{
  width: 50%;
  height: 250rpx;
  float: left;
  text-align: center;
}
.selectl_img{
  width: 50rpx;
  height: 50rpx;
  overflow: hidden;
  margin-top: 80rpx;
  margin-left: 50%;
  transform: translate(-50%);
}
.selectl_img image{
  width: 100%;
  height: 100%;
}
.select_l text{
  font-size:23rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:bold;
  color:rgba(255,255,255,1);
  line-height:50rpx;
  /* margin-left: 50%; */
}
.select_r{
  width: 50%;
  height: 250rpx;
  float: left;
  text-align: center;
}
.selectr_img{
  width: 50rpx;
  height: 50rpx;
  overflow: hidden;
  margin-top: 80rpx;
  margin-left: 50%;
  transform: translate(-50%);
}
.selectr_img image{
  width: 100%;
  height: 100%;
}
.select_r text{
  font-size:23rpx;
  font-family:SourceHanSansCN-Regular;
