  color:rgba(255,255,255,1);
  line-height:50rpx;
  /* margin-left: 50%; */
}
.send{
  width: 500rpx;
  height: 80rpx;
  background: #fff;
  font-size:25rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
  line-height:80rpx;
  text-align: center;
  border-radius:40rpx;
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  transform: translate(-50%);
}