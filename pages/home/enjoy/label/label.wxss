.tab_t{
  font-size:22rpx;
  margin: 0 40rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(221,221,221,1);
  line-height:80rpx;
}
.tab_c{
  margin: 30rpx 40rpx;
  overflow: hidden;
  background:rgba(0,0,0,0.2);
  border-radius:15rpx;
}
.tab_cl{
  float: left;
  width:60rpx;
  height:60rpx;
  background:rgba(0,0,0,0.6);
  border-radius:10rpx;
  margin: 30rpx;
  overflow: hidden;
}
.tab_cl image{
  width: 100%;
  height: 100%;
  display: block;
}
.tab_climg{
  width:36rpx;
  height:44rpx;
  margin: 8rpx 12rpx;
  overflow: hidden;
}
.tab_climg image{
  width: 100%;
  height: 100%;
}
.tab_cr{
  float: left;
  margin-left: 10rpx;
  width: 80%;
}
.tab_cr view{
  margin-top: 35rpx;
  font-size:25rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  /* line-height: 10rpx; */
  color:rgba(255,255,255,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.tab_cr text{
  font-size:21rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.more{
  background: #000;
  text-align: center;
  position: relative;
  height: 100rpx;
  
}
.more text{
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  font-size:25rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:100rpx;
}
.xiaimg{
  width: 20rpx;
  height: 10rpx;
  overflow: hidden;
  position: absolute;
