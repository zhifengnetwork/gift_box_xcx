.swiper-tab{
  margin-top: 200rpx
}
.about_page{
  margin: 0 10px;
}
.swiper-tab{
    text-align: center;
    height: 150rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    font-size: 16px;
}
.swiper_img{
  width: 90rpx;
  height: 90rpx;
  background:linear-gradient(0deg,rgba(156,145,142,1) 0%,rgba(77,77,76,1) 100%);
  border-radius:50%;
  overflow: hidden;
  margin-left: 50%;
  transform: translate(-50%);
  margin-top: 10rpx;
  z-index: 1;
}
.siteimg{
  width: 40rpx;
  height: 40rpx;
  overflow: hidden;
  margin-left: 50%;
  margin-top: 50%;
  transform: translate(-50%,-50%);
}
.siteimg image{
  width: 100%;
  height: 100%;
}
.swiper-tab-item{
    flex: 1;
   position: relative;
    /* background:linear-gradient(0deg,rgba(156,145,142,1) 0%,rgba(77,77,76,1) 100%); */
}
.touming{
  z-index: 999;
  width: 100%;
  height: 100%;
  background: yellow;
  position: absolute;
  opacity: 0;
}
.swiper-tab-item text{
  font-size:22rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(221,221,221,1);
  line-height:50rpx;
}
.active{
    color:black;
    /* border-bottom: 4rpx solid #F65959; */
    font-size: 35;
