/* 瀑布流 */
swiper {
  display: block !important;
  overflow: scroll !important;
}
.touxiang{
  margin-top: 10rpx;
  display: flex;
  align-items: center
}
.case-page {
  padding: 0 20rpx 20rpx 20rpx;
  margin-top: 20rpx
}

.list-masonry {
  column-count: 2;
  column-gap: 10rpx;
  height: 100%;
}

.item-masonry {
  background-color: #fff;
  break-inside: avoid;
  box-sizing: border-box;
  /* padding: 0  0rpx 20rpx 20rpx; */
  padding-bottom: 20rpx;
  font-size: 30rpx;
}

.item-masonry image {
  width: 100%;
}
.biaoti{
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.nike{
  margin-left: 20rpx;
  font-size: 24rpx;

}
.user_pic{
  display: inline-block;
  width: 50rpx!important;
  height: 50rpx;
  border-radius: 50%;
  background: red
}
.aixin_ear{
  width: 200rpx;
  height:50rpx;
  line-height: 50rpx;
