page{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.box{
  width: 100%;
  height: 100%;
}
.imgbox{
  z-index: 2;
  height: 100%;
  width: 100%;
  background: #fff;
  overflow: hidden;
}
.videobox{
  width: 100%;
  height: 100%;
}
.videobox video{
  width: 100%;
  height: 100%;
}
.popupimg{
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  z-index: 2;
}
.popupimg image{
  width: 100%;
  height: 100%;
  display: block;
}
.return{
  width: 30rpx;
  height: 30rpx;
  overflow: hidden;
  z-index: 3;
}
.return image{
  width: 100%;
  height: 100%;
  display: block;
}
.xbox{
  position: absolute;
  bottom: 150rpx;
  left: 40rpx;
  z-index: 3;
}
.touxiangimg{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  float: left;
  overflow: hidden;
}
.touxiangimg image{
  width: 100%;
  height: 100%;
  display: block;
}
.name{
  float: left;
  color: #fff;
  margin-left: 40rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}
.title{
  line-height: 120rpx;
  color: #fff;
  font-size: 32rpx;
}
.content{
  line-height: 120rpx;
  color: #fff;
  font-size: 32rpx;
}