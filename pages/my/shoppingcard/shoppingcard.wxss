.shoppingcard_nav{
  height: 115rpx;
}
.shoppingcard_top{
  height: 364rpx;
  background: #f4ead4;
  margin: 80rpx;
  border-radius: 12rpx;
}
.shoppingcard_topt{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #000;
  padding: 20rpx;
  position: relative;
}
.shoppingcard_topttext{
  margin-left: 60rpx;
}
.shoppingcard_border{
  height: 2rpx;
  background: #5b5540;
  position: absolute;
  top: 50rpx;
  right: 30rpx;
  width: 360rpx;
}
.shoppingcard_border:nth-child(1) {
  position: absolute;
  top: 50rpx;
  left: 30rpx;
  width: 40rpx;
}
.shoppingcard_topimg{
  width: 200rpx;
  height: 200rpx;
  background: #1e1e1e;
  border-radius: 50%;
  margin-left: 190rpx;
  line-height: 200rpx;
  font-size: 100rpx;
  text-align: center;
  color: #d5caa2;
  position: relative;
}
.shoppingcard_topjiu{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  border-radius: 50%;
  width: 25rpx;
  height: 25rpx;
  border: 1rpx solid #d5caa2;
  color: #d5caa2;
  line-height: 25rpx;
  text-align: center;
  font-size: 16rpx;
}
.shoppingcard_top_bottom{
  padding: 0 30rpx;
  height: 60rpx;
}
.shoppingcard_top_bottom>text{
  font-size: 26rpx;
  color: #908260;
  float: left;
  line-height: 60rpx;
}
.shoppingcard_top_bottom .shoppingcard_top_bottomr{
  float: right;
}
.shoppingcard_money{
  text-align: center;
  line-height: 166rpx;
  font-size: 28rpx;
  color: #222222;
  position: relative;
}
.shoppingcard_eye{
  width: 48rpx;
  height: 32rpx;
  background: #222;
  position: absolute;
  top: 66rpx;
  right: 222rpx;
}
.shoppingcard_bottom{
  margin: 20rpx 186rpx;
}
.shoppingcard_bottom>view{
  width: 350rpx;
  margin-bottom: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #222;
  border-radius: 30rpx;
  border: 1rpx solid #a9a7a7;
}
.shoppingcard_bottom .shoppingcard_recharge{
  background: #222;
  color: #fff;
}